#: main.c:79
#, c-format
msgid "Error initing preferences: %s"
msgstr "Fel vid initiering av inställningar: %s"

#: main.c:96 main.c:106
msgid ""
"Environment variable exists, but doesn't point to a folder containing scite."
msgstr ""
"Miljövariabeln finns, men hänvisar inte till någon mapp som innehåller scite."

#: main.c:123
#, c-format
msgid "Couldn't find a SciTE executable named '%s'!\n"
msgstr "Kunde inte hitta en körbar fil med namnet '%s'!\n"

#: main.c:124
msgid "Checking for SciTE in the standard locations instead.\n"
msgstr "Kollar efter SciTE på standard-platserna istället.\n"

#: main.c:137
msgid ""
"Warning! Couldn't locate SciTE!\n"
"Program will start, but you won't be able to open SciTE to edit files."
msgstr ""
"Varning! Kunde inte hitta SciTE!\n"
"Programmet kommer att starta, men du kommer inte kunna använda SciTE för att "
"editera filer."

#: main.c:145
#, c-format
msgid "Could not initialize application globals: %s"
msgstr "Kunde inte initiera program-globaler: %s"

#: main.c:210
#, c-format
msgid "Usage: %s [OPTION] [FILE]..."
msgstr "Användning: %s [FLAGGOR] [FIL]..."

#: main.c:214
msgid "Options can be one of the following:"
msgstr "Flaggor kan vara en av följande:"

#: main.c:215
msgid "  --help                            display this help and exit"
msgstr "  --help                            visa denna hjälp och avsluta"

#: main.c:216
msgid "  --version                         show version of sciteproj and exit"
msgstr ""
"  --version                         visa version av sciteproj och avsluta"

#: main.c:217
msgid ""
"  --scite FILENAME                  set a filename for the instance of SciTE "
"to open"
msgstr ""
"  --scite FILNAMN                   sätt ett filnamn för instansen av SciTE "
"du önskar köra"

#: main.c:218
msgid ""
"  --generate FILENAME [MAX_DEPTH]   generate a sciteproj project file with "
"name FILENAME,\n"
"                                    recursively from current folder "
"contents, at most\n"
"                                    MAX_DEPTH folders down in the hierarchy"
msgstr ""
"  --generate FILNAMN [MAX_DJUP]     generera en sciteproj-projektfil med "
"namn FILNAMN,\n"
"                                    rekursivt från aktuell folderinnehåll, "
"med som mest\n"
"                                    MAX_DJUP steg ner i mapp-hierarkin"

#: main.c:247
#, c-format
msgid "The syntax for that command is:"
msgstr "Syntaxen för det kommandot är:"

#: main.c:249
#, c-format
msgid "sciteproj --generate FILENAME [MAX_DEPTH]"
msgstr "sciteproj --generate FILNAMN [MAX_DJUP]"

#: main.c:264
#, c-format
msgid "Expected an integer as max_depth..."
msgstr "Väntade ett heltal för MAX_DJUP..."

#: main.c:276
#, c-format
msgid "Generated '%s' successfully!"
msgstr "Genererade '%s' framgångsrikt!"
